
drop table if exists memos;
drop table if exists counts;

create table memos (
    id integer primary key,
    user_id varchar(20),
    title varchar(20),
    content varchar(80),
    foreign key(user_id) references users(id)
);

create table counts (
    id integer primary key,
    counts integer
);


